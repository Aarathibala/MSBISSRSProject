  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdvWorkDB2</DataSourceName>
        <CommandText>Select B.ProductID ,B.ProductName,B.ProductCategory,B.ProductSubCategory,SOD.SalesOrderDetailID,SOD.OrderQty,SOD.LineTotal  from [Sales].[SalesOrderDetail] as SOD Inner Join

(Select Prd.ProductID,Prd.Name as 'ProductName', A.ProductName as 'ProductCategory',A.ProductSubCategory    from Production.Product as Prd 
									Inner Join (Select PrdCat.ProductCategoryID,PrdCat.Name as 'ProductName',PrdSubCat.ProductSubcategoryID,PrdSubCat.Name as 'ProductSubCategory' from Production.ProductSubcategory as PrdSubcat Inner Join  Production.ProductCategory as PrdCat on PrdSubCat.ProductCategoryID = PrdCat.ProductCategoryID ) as A on Prd.ProductSubcategoryID = A.ProductSubcategoryID ) as B On SOD.ProductID = B.ProductID 
</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductID">
          <DataField>ProductID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProductName">
